[user]
	name = YauHong
	email = tyauhong@gmail.com
[push]
	default = simple
[alias]
	st = status
	co = checkout
	pushme = push origin HEAD -u
	cm = commit
	br = branch
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = green
	changed = red
	untracked = cyan
[branch "master"]
	remote = origin
	merge = refs/heads/master
